@model Song

<h2>Create a New Song</h2>

@using (Html.BeginForm("InsertSongToDatabase", "Song", "Post"))
{
        <input asp-for="ID" type="hidden" value="" />

        <label asp-for="SongName" class="control-label">Song Name</label>
        <input type="text" name="SongName" class="form-control" value="" />

        <label asp-for="Artist" class="control-label">Artist</label>
        <input type="text" name="Artist" class="form-control" />
        <br />

    <input type="submit" value="Create Song" class="btn btn-outline-light" id="button" />
}